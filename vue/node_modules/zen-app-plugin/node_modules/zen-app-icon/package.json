{
  "name": "zen-app-icon",
  "version": "0.0.7",
  "description": "zen icon",
  "author": "zen",
  "license": "AAL",
  "main": "src/index.js",
  "scripts": {
    "deploy": "sh ./copy.sh",
    "start": "webpack-dev-server --mode=development",
    "dist": "npm run lint && webpack --mode=production",
    "init": "yarn",
    "lint": "eslint --fix --ext .js,.vue ./src"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/plugin-syntax-jsx": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/babel-preset-app": "^5.0.1",
    "@vue/compiler-sfc": "^3.2.36",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "cos-nodejs-sdk-v5": "^2.11.6",
    "css-loader": "^3.5.3",
    "eslint": "^7.28.0",
    "eslint-plugin-vue": "^8.4.1",
    "fs-extra": "^10.0.0",
    "lint-staged": "^12.3.4",
    "pako": "^2.0.4",
    "prettier": "^2.5.1",
    "prismjs": "^1.22.0",
    "sass": "^1.49.7",
    "sass-loader": "^12.4.0",
    "simple-git": "^3.1.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "vue": "^3.2.37",
    "vue-loader": "^17.0.0",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "eslintIgnore": [
    "public/*",
    "dist/*"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "plugins": [
      "vue"
    ],
    "parserOptions": {
      "parser": "babel-eslint",
      "sourceType": "module"
    },
    "globals": {
      "zen": true,
      "Vue": true,
      "$": true,
      "__webpack_public_path__": true
    },
    "rules": {
      "no-useless-escape": 0,
      "no-debugger": 0,
      "vue/multi-word-component-names": 0,
      "no-case-declaration": 0,
      "vue/no-mutating-props": 0
    }
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "cd vue && npm run lint"
    ]
  },
  "browserslist": [
    "last 1 chrome version",
    "last 1 firefox version",
    "last 1 safari version"
  ]
}